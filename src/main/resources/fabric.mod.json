{
  "schemaVersion": 1,
  "id": "mainmenuchanger",
  "version": "${version}",
  "name": "Main Menu Changer",
  "description": "Configurable mod that lets you change how the Main Menu is laid out.",
  "authors": ["Stereo528"],
  "contact": {
    "homepage": "https://stereo528.github.io",
    "repository": "https://github.com/Stereo528/MainMenuChanger",
    "issues": "https://github.com/Stereo528/MainMenuChanger/issues"
  },
  "license": "BSD-3-Clause",
  "icon": "assets/mainmenuchanger/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "io.github.stereo528.mainmenuchanger.client.MainMenuChangerClient"
    ],
    "modmenu": [
      "io.github.stereo528.mainmenuchanger.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "MainMenuChanger.mixins.json"
  ],
  "depends": {
    "minecraft": ">=1.19",
    "midnightlib": ">=1.0.0-fabric"
  },
  "recommends": {
    "modmenu": "*"
  }
}